{
  "name": "@runcitadel/sdk",
  "version": "0.3.8",
  "type": "module",
  "main": "dist/index.js",
  "browser": "browser/index.js",
  "scripts": {
    "start:node": "rollup --watch -c",
    "start:browser": "rollup --watch -c rollup.config.browser.js",
    "tsc:check": "tsc --noEmit",
    "lint": "eslint --fix . && prettier -w \"src/**/*.ts\"",
    "lint:check": "eslint . \"src/**/*.ts\"",
    "build": "rm -rf dist browser && rollup -c && rollup -c rollup.config.browser.js",
    "docs": "typedoc --out docs src/index.ts",
    "deps": "ncu -u",
    "prepublish": "yarn build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "npm-check-updates": "^12.5.4",
    "prettier": "^2.6.1",
    "react": "^18.0.0",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^2.0.5",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.3"
  },
  "optionalDependencies": {
    "@types/react": "^17.0.43",
    "react": "^18.0.0",
    "undici": "^5.0.0"
  },
  "packageManager": "yarn@3.2.0"
}
